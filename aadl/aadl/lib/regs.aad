}
$xn_ {
    xn XorSP; xn = #Rn; #Rd = xn
}
$xm {
    xm X; xm = #Rm; #Rm = xm
}
$xm_ {
    xm XorSP; xm = #Rm; #Rm = xm;
}
$xdnm {
    $xd; $xn; $xm
}
$xd_n_ {
    $xd_; $xn_;
}
$xd_n_m {
    $xd_; $xn_; $xm
}
