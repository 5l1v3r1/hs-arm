$wd {
    wd W; wd = #Rd; #Rd = wd
}
$wd_ {
    wd WorSP; wd = #Rd; #Rd = wd
}
$wn {
    wn W; wn = #Rn; #Rn = wn
}
$wn_ {
    wn WorSP; wn = #Rn; #Rn = wn
}
$wm {
    wm W; wm = #Rm; #Rm = wm
}
$wm_ {
    $wd; $wn; $wm
}
$wdnm {
    $wd; $wn; $wm
}
$wd_n_ {
    $wd_; $wn_;
}
$wd_n_m {
    $wd_; $wn_; $wm
}

