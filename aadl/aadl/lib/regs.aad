$wd {
    wd W in #Rd
}
$wD {
    wd WorSP in #Rd
}
$wn {
    wn W in #Rn
}
$wN {
    wn WorSP in #Rn
}
$wm {
    wm W in #Rm
}
$wM {
    $wd; $wn; $wm
}
$wdnm {
    $wd; $wn; $wm
}
$wDN {
    $wD; $wN;
}
$wDNm {
    $wD; $wN; $wm
}

$xd {
    xd X in #Rd
}
$xD {
    xd XorSP in #Rd
}
$xn {
    xn X in #Rn
}
$xN {
    xn XorSP in #Rd
}
$xm {
    xm X in #Rm
}
$xM {
    xm XorSP in #Rm
}
$xdnm {
    $xd; $xn; $xm
}
$xDN {
    $xD; $xN;
}
$xDNm {
    $xD; $xN; $xm
}
