// add_addsub_shift

insn ADD_32_addsub_shift { // ADD  <Wd>, <Wn>, <Wm>{, <shift> #<amount>}
    $wdnm
    imm Imm5
    imm = #imm6
    #imm6 = imm
    $shamsh
}

insn ADD_64_addsub_shift { // ADD  <Xd>, <Xn>, <Xm>{, <shift> #<amount>}
    $xdnm
    imm Imm6
    imm = #imm6
    #imm6 = imm
    $shamsh
}
