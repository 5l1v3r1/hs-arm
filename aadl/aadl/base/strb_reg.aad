// STRB  <Wt>, [<Xn|SP>, (<Wm>|<Xm>), <extend> {<amount>}]
insn STRB_32B_ldst_regoff {
    $wt; $xN; $rm
    extend Extend in option allowing {
        UXTW
        SXTW
        SXTX
    }
    enc S = 0
}

// STRB  <Wt>, [<Xn|SP>, <Xm>{, LSL <amount>}]
insn STRB_32BL_ldst_regoff {
    $wt; $xN; $xm
}
