// LDPSW  <Xt1>, <Xt2>, [<Xn|SP>], #<imm>
insn LDPSW_64_ldstpair_post {
    xt1 Xn in Rt
    xt2 Xn in Rt2
    $xn
    imm Int7 in imm7
}

// LDPSW  <Xt1>, <Xt2>, [<Xn|SP>, #<imm>]!
insn LDPSW_64_ldstpair_pre {
    xt1 Xn in Rt
    xt2 Xn in Rt2
    $xn
    imm Int7 in imm7
}

// LDPSW  <Xt1>, <Xt2>, [<Xn|SP>{, #<imm>}]
insn LDPSW_64_ldstpair_off {
    xt1 Xn in Rt
    xt2 Xn in Rt2
    $xn
    imm Int7 in imm7
}
