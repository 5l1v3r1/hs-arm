// LDRH  <Wt>, [<Xn|SP>], #<simm>
insn LDRH_32_ldst_immpost {
    $wt; $xN
    simm Int9 in imm9
}

// LDRH  <Wt>, [<Xn|SP>, #<simm>]!
insn LDRH_32_ldst_immpre {
    $wt; $xN
    simm Int9 in imm9
}

// LDRH  <Wt>, [<Xn|SP>{, #<pimm>}]
insn LDRH_32_ldst_pos {
    $wt; $xN
    pimm UInt12 in imm12
}
